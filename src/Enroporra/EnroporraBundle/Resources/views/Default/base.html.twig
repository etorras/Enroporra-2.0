<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>{% block titulo %}Enroporra - {{ nombre_competicion }}{% endblock %}</title>
<link href="{{ asset('bundles/enroporra/css/style.css') }}" rel="stylesheet" type="text/css" />
<link href="{{ asset('bundles/enroporra/css/headers' ~ (random(11)+1) ~ '.css') }}" rel="stylesheet" type="text/css" />
</head>
<body>
    {{ base.debug|raw }}
<div id="container">
	<!-- header -->
    <div id="header">
    	<div id="logo"><a href="/">&nbsp;</a></div>
        <div id="menu">
            {% block menu %}{% endblock %}
        </div>
  </div>
    <!--end header -->
    <!-- main -->
    <div id="main">
    	<div id="content">
			<div style='float:right'>
				<a href="https://twitter.com/comisionporra" class="twitter-follow-button" data-show-count="false" data-lang="es" data-size="large">Seguir a @comisionporra en Twitter</a>
				<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
			</div>
			<div style='clear:both; height: 30px;'></div>
                            {% block proximos_partidos %}{% endblock %}
                            {% block mensajes_extra %}{% endblock %}
            <div id="content-left">
                {% block cuerpo %}{% endblock %}
            </div>

            <div id="content-right">
                {% block columna_derecha %}{% endblock %}
            </div>

            <div style='clear:both; height: 30px;'></div>
        </div>
    </div>
    <!-- end main -->
    <!-- footer -->
    <div id="footer">
    <div id="left_footer">Copyright 1994-{{ "now"|date("Y") }} Enroporra
    </div>
    <div id="right_footer">
    </div>
    </div>
    <!-- end footer -->
</div>
<div style="text-align: center; font-size: 0.75em;">Design downloaded from <a href="http://www.freewebtemplates.com/">free website templates</a>.</div>
</body>
</html>
